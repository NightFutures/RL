{
    "dns": {
        "servers": [
            {
                "tag": "dns_domain-proxy",
                "address": "dns.google",
                "domains": [
                    "domain:jut.su",
                    "domain:animejoy.ru"
                ],
                "skipFallback": true
            },
            {
                "tag": "dns_domain-WARP",
                "address": "one.one.one.one",
                "domains": [
                    "domain:habr.com",
                    "domain:2ip.ru",
                    "domain:2ip.io",
                    "domain:shikimori.one"
                ],
                "skipFallback": true
            },
            {
                "tag": "dns_domain-direct",
                "address": "localhost",
                "domains": [
                    "domain:cdn.adfinity.pro",
                    "domain:livejournal.com",
                    "domain:ranobelib.me",
                    "domain:static-storage.net",
                    "domain:animego.org",
                    "domain:timeweb.cloud",
                    "domain:timeweb.com",
                    "domain:roistat.com",
                    "domain:secvideo1.online",
                    "domain:novel.tl"
