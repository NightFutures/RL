            {
                "tag": "dns_else-proxy",
                "address": "dns.google",
                "skipFallback": false
            }
        ]
    },
    "routing": {
        "rules": [
            {
                "domain": [
                    "domain:jut.su",
                    "domain:animejoy.ru"
                ],
                "outboundTag": "outbound_proxy",
                "ruleTag": "route_domain-proxy"
            },
            {
                "domain": [
                    "domain:habr.com",
                    "domain:2ip.ru",
                    "domain:2ip.io",
                    "domain:shikimori.one"
                ],
                "outboundTag": "outbound_proxy",
                "ruleTag": "route_domain-WARP"
            },
            {
                "protocol": [
                    "bittorrent"
                ],
                "outboundTag": "outbound_direct",
                "ruleTag": "route_protocol-direct"
            },
            {
                "domain": [
                    "domain:cdn.adfinity.pro",
                    "domain:livejournal.com",
                    "domain:ranobelib.me",
                    "domain:static-storage.net",
                    "domain:animego.org",
                    "domain:timeweb.cloud",
                    "domain:timeweb.com",
                    "domain:roistat.com",
                    "domain:secvideo1.online",
                    "domain:novel.tl"
                ],
