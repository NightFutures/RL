                "ruleTag": "route_domain-direct"
            },
            {
                "domain": [
                    "geosite:private",
                    "geosite:category-ru",
                    "geosite:cn"
                ],
                "outboundTag": "outbound_direct",
                "ruleTag": "route_geosite-direct"
            },
            {
                "ip": [
                    "geoip:private",
                    "geoip:ru",
                    "geoip:cn"
                ],
                "outboundTag": "outbound_direct",
                "ruleTag": "route_geoip-direct"
            },
            {
                "network": "tcp,udp",
                "outboundTag": "outbound_proxy",
                "ruleTag": "route_else-proxy"
            }
        ]
    }
}
